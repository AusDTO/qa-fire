= render 'application/head', title: (content_for?(:title) ? "#{yield(:title)} | QA-Fire" : 'QA-Fire' )
%body
  %div.skip-to
    %a{href: "#content"}
      Skip to main content
  %header{role: 'banner'}

    %section.prerelease-govau--header
      - if content_for?(:header)
        = yield(:header)
      - else
        .wrapper
          %a.govau--logo.logo{href: "/"}
            QA-Fire

        %nav.breadcrumbs--inverted{'aria-label' => 'breadcrumb'}
          .wrapper
            = breadcrumbs style: :ul, class: nil

  %main{role: 'main'}
    %article#content.content-main{tabindex: -1}
      = render 'application/alerts'
      = yield
  %footer{role: "contentinfo"}
    .wrapper
      %section
        .footer-links
          %nav
            = render 'application/footer_links'

          %p
            &copy; Commonwealth of Australia
            %br
            %a{href:'http://creativecommons.org/licenses/by/3.0/au/'} This work is licensed under a Creative Commons Attribution 3.0 International License
</html>
